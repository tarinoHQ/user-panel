{
  "presets": ["es2015", "react", "stage-0"],
  "passPerPreset": true,
	"presets": [
		{
      "plugins": [["transform-runtime", {
        "polyfill": true,
        "regenerator": true
      }]]
    },
		{
			"passPerPreset": false,
			"presets": ["es2015", "react", "stage-0"]
		}
	],
  "plugins": [
    "transform-decorators-legacy",
    "transform-regenerator",
   ],
  "env": {
    "development": {
      "plugins": ["react-hot-loader/babel"]
    },
    "production": {
      "plugins": [
        "transform-react-remove-prop-types",
        "transform-react-inline-elements"
      ]
    }
  }
}
